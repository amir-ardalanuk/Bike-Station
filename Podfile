# Uncomment the next line to define a global platform for your project
 platform :ios, '9.0'

target 'BikeApp' do
   use_frameworks!
  pod 'RxSwift', '~> 5'
  pod 'RxCocoa', '~> 5'
  pod 'Swinject'
  pod 'SwinjectStoryboard'
 pod 'GoogleMaps'
 # Pods for UI in Code
  target 'NetworkPlatform' do
       inherit! :search_paths
    end
  
  target 'Domain' do
    inherit! :search_paths
  end
  
   target 'RemotePlatform' do
      inherit! :search_paths
    end
  
end
